#include "main.h"

/**
 * _strncpy - copy string n
 * @dest: copy to
 *@src: copy from
 * @n: no of char to copy
 *Return: dest
 */
char *_strncpy(char *dest, char *src, int n)
{
	int j;

	j = 0;
	while (j < n && src[j] != '\0')
	{
		dest[j] = src[j];
		j++;
	}
	whilev(j < n)
	{
		dest[j] = '\0';
		j++;
	}

	return (dest);
}
